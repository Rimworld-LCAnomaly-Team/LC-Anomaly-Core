<?xml version="1.0" encoding="UTF-8" ?>
<Defs>

  <!-- Convert Enkephalin To Bioferrite x10 -->
  <RecipeDef>
    <defName>ConvertEnkephalinToBioferrite10</defName>
    <label>Convert Enkephalin To Bioferrite x10</label>
    <description>.</description>
    <jobString>Converting Bioferrite.</jobString>
    <workAmount>4000</workAmount>
    <workSpeedStat>DrugSynthesisSpeed</workSpeedStat>
    <soundWorking>Recipe_Drug</soundWorking>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>EnkephalinBox</li>
          </thingDefs>
        </filter>
        <count>20</count>
      </li>
    </ingredients>
    <products>
      <Bioferrite>10</Bioferrite>
    </products>
	<recipeUsers>
		<li>LC_UniversalBench</li>
	</recipeUsers>
  <researchPrerequisite>EnkephalinConvert</researchPrerequisite>
	<skillRequirements>
    <Crafting>5</Crafting>
    <Intellectual>5</Intellectual>
	</skillRequirements>
  <workSkill>Intellectual</workSkill>
  </RecipeDef>

