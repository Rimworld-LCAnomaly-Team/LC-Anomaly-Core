  <!-- WorkResult_Good -->
  <FleckDef ParentName="FleckBase_Thrown">
    <defName>WorkResult_Good</defName>
    <altitudeLayer>MoteOverhead</altitudeLayer>
    <fadeInTime>0.2</fadeInTime>
    <solidTime>1.8</solidTime>
    <fadeOutTime>0.2</fadeOutTime>
    <growthRate>0.0</growthRate>
    <graphicData>
      <graphicClass>Graphic_FleckPulse</graphicClass>
      <shaderType>TransparentAnimated</shaderType>
      <texPath>Fleck/WorkResult_Good</texPath>
      <drawSize>(4, 4)</drawSize>
      <shaderParameters>
        <_NumFrames>22</_NumFrames>        <!--一共几帧-->
        <_FramesPerSec>10</_FramesPerSec>        <!--一秒几帧-->
      </shaderParameters>
    </graphicData>
  </FleckDef>

